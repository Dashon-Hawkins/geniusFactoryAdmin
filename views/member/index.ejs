  <div class="container">
  	<h3> Members </h3>
  	<table class='table' id="membersListTable">
  		<tr>
        <th>Member Online</th>
  			<th>ID</th>
  			<th>firstName</th>
        <th>lastName</th>
  			<th>Title</th>
  			<th>Email</th>
  			<th>zipCode</th>
  			<th>Admin</th>
  		</tr>

  		<% _.each(member, function(member) { %>
  		<tr data-id="<%= member.id %>" data-model="member">
        <% if (member.online) { %>
      <td><img src="/images/icon-online.png" class="online-img"></td>
    <% } else { %>
      <td> <img src="/images/icon-offline.png" class="offline-img"></td>
    <% } %>

  			<td><%= member.id %></td>
  			<td><%= member.firstName %></td>
        <td><%= member.lastName %></td>
  			<td><%= member.title %></td>
  			<td><%= member.email %></td>
        <td><%= member.zipCode %></td>
          <% if (member.admin) { %>
            <td> <img src="/images/admin.png" class="admin-img"></td>
            <% } else { %>
              <td> <img src="/images/pawn.png" class="member-img"></td>
              <% } %>
  			<td><a href="/member/show/<%= member.id %>" class="btn btn-sm btn-primary">Show</a></td>
  			<td><a href="/member/edit/<%= member.id %>" class="btn btn-sm btn-warning">Edit</a></td>
  			<td><form action="/member/destroy/<%= member.id %>" method="POST">
  				<input type="hidden" name="_method" value="delete"/>
  				<input type="submit" class="btn btn-sm btn-danger" value="Delete"/>
  				<input type="hidden" name="_csrf" value="<%= _csrf %>" />
  			</form></td>
      </tr>

  		<% }) %>
  	</table>
